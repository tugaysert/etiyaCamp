package intro;

import java.util.Iterator;

public class Main {

	public static void main(String[] args) {
		
		
		
		for (int i = 2; i <= 1000; i++) {
			
			if(isPrime(i)==true) {
				System.out.println(i);
			}
		
		}
				
		
		
	}
	
	public static boolean isPrime(int num) {
		boolean isPrime = true;
		   for (int i = 2; i <= 1000; i++){
	            if ( num % i == 0 ){
	                isPrime = false;
	            }
	        }
		   return isPrime;
				   
	}

}
